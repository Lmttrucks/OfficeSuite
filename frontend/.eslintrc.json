{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
    "extends": [
      "react-app",
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "plugins": ["react"],
    "rules": {
      "no-console": "off",
      "comma-dangle": ["error", "never"],
      "no-unused-vars": "off"
    }
  }
